{
	"compilerOptions": {
		"module": "commonjs",
		"outDir": "dist",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"noEmitOnError": false,
		"target": "es6",
		"lib": ["es2019"],
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noUnusedLocals": false,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"strict": true
	},
	"include": [
		"routes",
		"structures",
		"server.ts"
	]
}